<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tavio_syrus_gblokpo.iai_vote.ListEtud">


<android.support.v7.widget.CardView
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:onClick="detail"
    android:backgroundTint="#ffff">



<RelativeLayout
    android:id="@+id/rellist"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <android.support.v7.widget.CardView
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/cardView2">
        <com.example.tavio_syrus_gblokpo.iai_vote.Traitement.CircleImageView
            android:id="@+id/photo_util"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:src="@mipmap/ic_ins_round">

        </com.example.tavio_syrus_gblokpo.iai_vote.Traitement.CircleImageView>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/cardView2"
        android:backgroundTint="@color/cardview_light_background">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="65dp">

        <TextView
            android:id="@+id/idEt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/nomEt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:textAllCaps="true"
            android:textStyle="italic"
           style="@style/AppTheme"
            android:text="nom" />



</RelativeLayout>

    </android.support.v7.widget.CardView>



</RelativeLayout>





</android.support.v7.widget.CardView>


</android.support.constraint.ConstraintLayout>
